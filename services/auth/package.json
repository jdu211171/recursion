{
  "name": "auth",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun src/index.ts",
    "lint": "eslint . --ext .ts",
    "build": "bun build src/index.ts --outfile dist/index.js",
    "seed": "bun run prisma/seed.ts",
    "seed:reset": "bun prisma migrate reset --force && bun run prisma/seed.ts",
    "seed:small": "SEED_SIZE=small bun run prisma/seed.ts",
    "seed:medium": "SEED_SIZE=medium bun run prisma/seed.ts",
    "seed:large": "SEED_SIZE=large bun run prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "latest",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "prisma": "^6.12.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@types/cors": "^2.8.19",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  }
}
