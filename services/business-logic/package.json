{
  "name": "business-logic",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun src/index.ts",
    "dev": "bun --watch src/index.ts",
    "lint": "eslint . --ext .ts",
    "build": "bun build src/index.ts --outfile dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "seed": "bun run prisma/seed.ts",
    "seed:reset": "bun prisma migrate reset --force && bun run prisma/seed.ts",
    "seed:small": "SEED_SIZE=small bun run prisma/seed.ts",
    "seed:medium": "SEED_SIZE=medium bun run prisma/seed.ts",
    "seed:large": "SEED_SIZE=large bun run prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "prisma": "^6.12.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "uuid": "^11.1.0"
  }
}
